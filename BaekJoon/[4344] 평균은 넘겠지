# 내가 생각한것
import sys

C = int(input())
a = list(map(int, sys.stdin.readline().split())

avg = sum(a)/a[0]

cnt = 0

while True :
  if  > avg :
    cnt += 1
  
  print(cnt/a[0] * 100)
  

# 풀이 
import sys
C = int(input())

for _ in range(C) :
    a = list(map(int, sys.stdin.readline().split())
    avg = sum(a[1:])/a[0] 
    cnt = 0
    
    for score in a[1:] :
        if score > avg :
            cnt += 1
        
